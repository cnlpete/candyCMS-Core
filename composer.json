{
  "name": "candycms/core",
  "type": "library",
  "description": "Core elements for candyCMS.",
  "keywords": ["php", "candy", "cms"],
  "homepage": "http://www.candycms.com",
  "license": "MIT",

  "repositories":
    [
      {
        "type": "package",
        "package": {
          "name": "recaptcha/recaptcha",
          "version": "1.11",
          "dist": {
            "url": "http://recaptcha.googlecode.com/files/recaptcha-php-1.11.zip",
            "type": "zip"
          },
          "source": {
            "url": "http://recaptcha.googlecode.com/svn/",
            "type": "svn",
            "reference": "trunk/recaptcha-plugins/php/"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "smushit/smushit",
          "version": "1.0",
          "dist": {
            "url": "https://github.com/marcoraddatz/smushit-php/zipball/master",
            "type": "zip"
          },
          "source": {
            "url": "https://github.com/marcoraddatz/smushit-php",
            "type": "git",
            "reference": "master"
          }
        }
      }
    ],

  "require": {
    "php": ">=5.3.0",

    "smarty/smarty": "3.1.*",
    "symfony/yaml": "2.2.*",
    "twitter/bootstrap" : "2.3.*",
    "leafo/lessphp": "0.3.*",
    "simonhamp/routes": "1.*",
    "cnlpete/image-metadata-parser" : "0.0.*",
    "imagine/imagine": "0.5.*",
    "phpmailer/phpmailer": "5.2.*",
    "mailchimp/mailchimp": "2.0.*",

    "smushit/smushit": "1.0.*",
    "recaptcha/recaptcha": "1.*"
  },

  "authors": [ {
    "name": "Marco Raddatz",
    "homepage": "http://www.marcoraddatz.com"
  }, {
    "name": "Hauke Schade",
    "homepage": "http://www.hauke-schade.de/"
  }],

  "autoload": {
    "psr-0": {
      "candyCMS\\Core\\Index": ""
    }
  },

  "target-dir": ""
}